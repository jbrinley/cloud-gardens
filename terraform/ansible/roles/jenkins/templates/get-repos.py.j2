import requests
import json
import base64

authBasic  = base64.b64encode(b'{{ github_username }}:{{ github_password }}')
r = requests.get('https://api.github.com/repos/{{ github_organization }}?per_page=1000', headers={'Authorization': 'Basic ' + authBasic})
if (r.ok):
  repoItem = json.loads(r.text or r.content)
  print "Django repository created: " + repoItem['created_at']